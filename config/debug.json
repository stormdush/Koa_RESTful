{
    "port": "3000",
    "env": "debug",
    "https": true,
    "secret": "secret",
    "mongoDB": {
        "host": "127.0.0.1",
        "port": 27017,
        "db": "test"
    },
    "session": {
        "key": "KOA RESTful",
        "maxAge": 86400000,
        "autoCommit": true,
        "overwrite": true,
        "httpOnly": true,
        "signed": true,
        "rolling": false,
        "renew": true,
        "secure": true,
        "sameSite": null
    },
    "publicCache": "/static",
    "logger": {
        "path": "logs",
        "levels": {
            "error": 0,
            "warn": 1,
            "info": 2,
            "debug": 3
        },
        "transportsetting": {
            "error": {
                "filename": "logs/%DATE%-error.log",
                "level": "error",
                "datePattern": "YYYY-MM-DD",
                "zippedArchive": true,
                "maxSize": "20m",
                "maxFiles": "7d"
            },
            "warn": {
                "filename": "logs/%DATE%-warn.log",
                "level": "warn",
                "datePattern": "YYYY-MM-DD",
                "zippedArchive": true,
                "maxSize": "20m",
                "maxFiles": "7d"
            },
            "info": {
                "filename": "logs/%DATE%-info.log",
                "level": "info",
                "datePattern": "YYYY-MM-DD",
                "zippedArchive": true,
                "maxSize": "20m",
                "maxFiles": "7d"
            },
            "debug": {
                "filename": "logs/%DATE%-debug.log",
                "level": "debug",
                "datePattern": "YYYY-MM-DD",
                "zippedArchive": true,
                "maxSize": "20m",
                "maxFiles": "7d"
            }
        }
    }
}